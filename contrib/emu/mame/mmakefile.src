include $(SRCDIR)/config/aros.cfg

AROSMAME_USEZLIB			:= "yes"

ifeq ($(AROSMAME_USEZLIB),"yes")
    ZLIB 				:= z
    AROSMAME_EXTRADEFS			:= -DAROSMAME_USEZLIB
else
    ZLIB 				:=
    AROSMAME_EXTRADEFS			:=
endif

INSTDIR  				:= $(AROS_CONTRIB)/Emu/Mame
OBJDIR   				:= $(GENDIR)/$(CURDIR)

EXE 					:= $(INSTDIR)/mame

#MM- contrib-emu : Mame
#MM
Mame : setup $(EXE)

setup : make-dirs maketree
	
make-dirs :
	%mkdirs_q $(INSTDIR) $(OBJDIR) $(INSTDIR)/roms $(INSTDIR)/snap

#MM MameSourceFix
MameSourceFix: $(TOOLDIR)/stripcr
	@find . -name "*.c" -or -name "*.h" -or -name "*.txt" | xargs $(TOOLDIR)/stripcr
	
$(TOOLDIR)/stripcr: $(SRCDIR)/$(CURDIR)/src/aros/stripcr/stripcr.c
	@$(HOST_CC) -s $< -o $@ 

#ASM = nasm
ASM = nasmw
ASMFLAGS = -f coff
VPATH=src $(wildcard src/cpu/*)

EMULATOR_EXE = $(EXE)

